pragma solidity ^0.5.0;

/* Assignment 3
a. Create functions that does the following:
Add 5 and 2 to state variable
b. Subtract 2 from the state variable */

contract Ifure3{

    uint public count;

    function add() public {

    count +=5;
    count +=2;

    }

    function subtract() public {

        count -=2;
    }
}
